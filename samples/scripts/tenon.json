{
  "what": "Test example.com with tenon",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "Chromium browser"
    },
    {
      "type": "url",
      "which": "https://example.com/",
      "what": "page with a few accessibility defects"
    },
    {
      "type": "tenonRequest",
      "withNewContent": true,
      "id": "a",
      "what": "tenon request"
    },
    {
      "type": "test",
      "which": "tenon",
      "id": "a",
      "what": "result of prior tenon request"
    }
  ]
}
