{
  "id": "tenon",
  "what": "Test Wikipedia with tenon",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "Chromium browser"
    },
    {
      "type": "url",
      "which": "https://en.wikipedia.org/wiki/Main_Page",
      "what": "Wikipedia English home page"
    },
    {
      "type": "tenonRequest",
      "withNewContent": true,
      "id": "a",
      "what": "tenon request"
    },
    {
      "type": "test",
      "which": "tenon",
      "id": "a",
      "what": "result of prior tenon request"
    }
  ]
}
