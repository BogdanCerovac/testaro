{
  "script": {
    "id": "simple",
    "what": "Test example.com with bulk",
    "strict": true,
    "commands": [
      {
        "type": "launch",
        "which": "chromium",
        "what": "Chromium browser"
      },
      {
        "type": "url",
        "which": "https://example.com/",
        "what": "page with a few accessibility defects"
      },
      {
        "type": "test",
        "which": "bulk",
        "what": "bulk"
      }
    ]
  },
  "batch": {
    "id": "weborgs",
    "what": "Web organizations",
    "hosts": [
      {
        "id": "mozilla",
        "which": "https://www.mozilla.org/en-US/",
        "what": "Mozilla"
      },
      {
        "id": "w3c",
        "which": "https://www.w3.org/",
        "what": "W3C"
      }
    ]
  }
}
