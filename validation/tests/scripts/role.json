{
  "what": "validation of role test",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "__targets__/role/good.html",
      "what": "page with standard roles"
    },
    {
      "type": "test",
      "which": "role",
      "what": "role",
      "expect": [
        ["roleElements", "=", 1],
        ["badRoleElements", "=", 0]
      ]
    },
    {
      "type": "url",
      "which": "__targets__/role/bad.html",
      "what": "page with deviant roles"
    },
    {
      "type": "test",
      "which": "role",
      "what": "role",
      "expect": [
        ["roleElements", "=", 3],
        ["badRoleElements", "=", 3],
        ["tagNames.SECTION.main", "=", 1],
        ["tagNames.SECTION.section", "=", 1],
        ["tagNames.H2.heading", "=", 1]
      ]
    }
  ]
}
