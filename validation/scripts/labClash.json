{
  "what": "validation of labClash test",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "file://__dirname/validation/targets/labClash/good.html",
      "what": "Page with standard labeling"
    },
    {
      "type": "test",
      "which": "labClash",
      "what": "Labeling",
      "withItems": false,
      "expect": [
        ["totals.mislabeled", "=", 0],
        ["totals.unlabeled", "=", 0],
        ["totals.wellLabeled", "=", 5]
      ]
    },
    {
      "type": "url",
      "which": "file://__dirname/validation/targets/labClash/bad.html",
      "what": "partly Tab-focusable page"
    },
    {
      "type": "test",
      "which": "focAll",
      "what": "Tab-focusability",
      "withItems": false,
      "expect": [
        ["totals.mislabeled", "=", 3],
        ["totals.unlabeled", "=", 2],
        ["totals.wellLabeled", "=", 0]
      ]
    }
  ]
}
