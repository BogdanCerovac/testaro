{
  "what": "tests of bulk test",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "chromium",
      "what": "usual browser"
    },
    {
      "type": "url",
      "which": "file://__dirname/validation/targets/bulk/good.html",
      "what": "small page"
    },
    {
      "type": "test",
      "which": "bulk",
      "what": "visible element count",
      "expect": [
        ["visibleElements", ">", 2],
        ["visibleElements", "<", 10]
      ]
    },
    {
      "type": "url",
      "which": "file://__dirname/validation/targets/bulk/bad.html",
      "what": "large page"
    },
    {
      "type": "test",
      "which": "bulk",
      "what": "visible element count",
      "expect": [
        ["visibleElements", ">", 30],
        ["visibleElements", "<", 50]
      ]
    }
  ]
}
